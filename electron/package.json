{
  "name": "calculator-sample-electron",
  "version": "0.0.0",
  "main": "main.js",
  "description": "",
  "scripts": {
    "node-gyp:configure": "node-gyp configure",
    "node-gyp:clean": "node-gyp clean",
    "node-gyp:build": "node-gyp build",
    "node-gyp:rebuild": "node-gyp rebuild",
    "angular:build": "angular_build",
    "electron-rebuild": "electron-rebuild .",
    "electron": "electron .",
    "start": "electron .",
    "electron-builder:build": "electron-builder build --projectDir .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.example.app",
    "directories": {
      "buildResources": "builder",
      "output": "builder/dist"
    },
    "nodeGypRebuild": true,
    "files": [
      "**/dist/*",
      "**/hello.node",
      "**/calculator-intf.node",
      "**/main.js",
      "**/package*",
      "!*/bin/*",
      "!*/angular_build*",
      "!*/include/*",
      "!*/src/*"
    ],
    "asar": true
  },
  "private": true,
  "keywords": [],
  "author": "Ananta Yudica",
  "license": "MIT",
  "devDependencies": {
    "electron": "11.1.1",
    "electron-builder": "22.9.1",
    "electron-rebuild": "2.3.4",
    "node-gyp": "7.1.2",
    "node-addon-api": "3.1.0"
  }
}
